    <!-- How It Works -->
    <section class="how-it-works">
        <div class="container">
            <div class="section-intro">
                <h2 class="section-title">From <span>social posts</span> to sales in minutes</h2>
                <p class="section-description">A simple 3-step process designed for busy creative entrepreneurs</p>
            </div>
            <div class="process-steps">
                <div class="process-connector"></div>
                <div class="process-step">
                    <div class="step-number">1</div>
                    <div class="step-content">
                        <h3 class="step-title">Connect your Instagram</h3>
