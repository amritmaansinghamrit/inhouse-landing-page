    <!-- SCCG Framework -->
    <section class="sccg-framework">
        <div class="container">
            <div class="framework-container">
                <div class="framework-content">
                    <h2 class="framework-title">The <span>SCCG</span> Framework</h2>
                    <p class="framework-description">Our Simplified Customer Commerce Growth approach helps small businesses stay small but grow sustainably</p>
                    <ul class="framework-list">
                        <li class="framework-item">
                            <div class="framework-item-icon icon-orange">S</div>
                            <div class="framework-item-content">
                                <h3>Simplified</h3>
                                <p>Zero technical setup. No coding. No complicated tools. Just connect your Instagram and start selling.</p>
                            </div>
                        </li>
                        <li class="framework-item">
                            <div class="framework-item-icon icon-blue">C</div>
                            <div class="framework-item-content">
                                <h3>Customer-Focused</h3>
                                <p>Shopping experiences that feel natural and personal through channels your customers already love.</p>
                            </div>
                        </li>
                        <li class="framework-item">
                            <div class="framework-item-icon icon-yellow">G</div>
                            <div class="framework-item-content">
                                <h3>Growth Intelligence</h3>
                                <p>Smart insights that tell you exactly what products to promote, when to restock, and how to price for profit.</p>
                            </div>
                        </li>
                    </ul>
                </div>
